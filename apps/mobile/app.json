{
  "expo": {
    "name": "MeuPersonal",
    "slug": "MeuPersonal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "meupersonal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSHealthShareUsageDescription": "Precisamos acessar seus dados de saúde para sincronizar passos e calorias.",
        "NSHealthUpdateUsageDescription": "Precisamos acessar seus dados de saúde para salvar seus treinos."
      },
      "bundleIdentifier": "com.anonymous.MeuPersonal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.MeuPersonal",
      "permissions": [
        "SCHEDULE_EXACT_ALARM",
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED",
        "android.permission.health.WRITE_STEPS",
        "android.permission.health.WRITE_ACTIVE_CALORIES_BURNED"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": false
        }
      ],
      [
        "react-native-health-connect",
        {
          "rationaleActivityName": "com.anonymous.MeuPersonal.RationaleActivity"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "root": "src/app"
      },
      "eas": {
        "projectId": "cfcc0b23-987d-4039-9679-dab9739f6552"
      }
    },
    "owner": "daniellevi98"
  }
}